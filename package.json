{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Express typescript boilerplate - Mattecurve",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-package && npm run copy-config",
    "build:prod": "npm run build",
    "clean": "shx rm -rf ./dist",
    "copy-package": "shx cp ./package.json ./dist/ && shx cp ./package-lock.json ./dist/",
    "copy-config": "shx cp -R ./src/config ./dist/",
    "live-reload": "nodemon -e js",
    "test": "NODE_ENV=testing mocha",
    "lint": "eslint . --ext .ts",
    "start": "cd dist && node index.js",
    "dev": "npm run build && tsc --project './tsconfig.json' --watch & NODE_ENV=development cd dist && nodemon",
    "dev:rpc": "tsc --project './tsconfig.json' --watch & NODE_ENV=development cd dist && nodemon rpc.index.js",
    "command": "tsc --project './tsconfig.json' & NODE_ENV=development cd dist && node crons/index.js",
    "dev:cluster": "",
    "migration": "migrate-mongo",
    "migration:help": "migrate-mongo"
  },
  "author": "onkar@mattecurve.com",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.95.0",
    "amqplib": "^0.10.0",
    "axios": "^0.27.2",
    "basic-ftp": "^4.6.6",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "compression": "^1.7.4",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "express-winston": "^4.2.0",
    "firebase-admin": "^11.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "migrate-mongo": "^9.0.0",
    "mime-types": "^2.1.35",
    "mocha": "^9.2.2",
    "mongoose": "^6.2.10",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.5",
    "otp-generator": "^4.0.0",
    "pm2": "^5.2.0",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "remote-procedure-call-rabbitmq": "^1.0.11",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/compression": "^1.7.2",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.3.2",
    "@types/migrate-mongo": "^8.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^9.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/otp-generator": "^4.0.0",
    "@types/randomatic": "^3.1.3",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^1.3.3",
    "prettier": "^2.6.2",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
